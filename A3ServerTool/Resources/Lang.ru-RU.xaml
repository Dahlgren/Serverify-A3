<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="LocalizableCurrentProfileTitle">Текущий профиль:</v:String>

    <v:String x:Key="LocalizableSaveButton">Сохранить</v:String>

    <v:String x:Key="LocalizableOn">Вкл</v:String>
    <v:String x:Key="LocalizableOff">Выкл</v:String>
    <v:String x:Key="LocalizableEnterSqfTitle">Вводить SQF-код</v:String>

    <v:String x:Key="LocalizableGeneralTabTitle">Основное</v:String>
    <v:String x:Key="LocalizableSecurityTabTitle">Безопасность</v:String>
    <v:String x:Key="LocalizableNetworkTabTitle">Сеть</v:String>
    <v:String x:Key="LocalizableLoggingTabTittle">Логгирование</v:String>
    <v:String x:Key="LocalizablePerformanceTabTittle">Производительность</v:String>
    <v:String x:Key="LocalizableDifficultyTabTittle">Сложность</v:String>
    <v:String x:Key="LocalizableModificationsTabTittle">Моды</v:String>
    <v:String x:Key="LocalizableMissionsTabTittle">Миссии</v:String>

    <v:String x:Key="LocalizableSettingsHeader">Настройки</v:String>
    <v:String x:Key="LocalizableSettingsLanguageTitle">Язык</v:String>

    <!--  Server view  -->
    <v:String x:Key="LocalizableServerViewTitle">Сервер</v:String>
    <v:String x:Key="LocalizableLaunchButtonTitle">Запуск</v:String>

    <!--  General view  -->
    <v:String x:Key="LocalizableCommonSettingsGroupTitle">Общие настройки</v:String>
    <v:String x:Key="LocalizablePlayerInteractionsGroupTitle">Взаимодействие с игроками</v:String>
    <v:String x:Key="LocalizableHeadlessClientGroupTitle">Headless-клиент</v:String>
    <v:String x:Key="LocalizableVonGroupTitle">Голосовая связь</v:String>
    <v:String x:Key="LocalizableMiscGroupTitle">Прочее</v:String>

    <v:String x:Key="LocalizableServerNameTitle">Имя сервера</v:String>
    <v:String x:Key="LocalizableServerNameWatermark">Имя в браузере серверов</v:String>

    <v:String x:Key="LocalizablePasswordTitle">Пароль</v:String>
    <v:String x:Key="LocalizablePasswordWatermark">Пароль для присоединения к серверу</v:String>

    <v:String x:Key="LocalizableMaxPlayersTitle">Количество игроков</v:String>
    <v:String x:Key="LocalizableMaxPlayersTooltip">Максимальное количество игроков, которые могут присоединиться к серверу.</v:String>

    <v:String x:Key="LocalizableServerExecLocationTitle">Местоположение файла сервера</v:String>
    <v:String x:Key="LocalizableServerExecWatermark">Путь до исполняемого файла сервера</v:String>

    <v:String x:Key="LocalizableWelcomeMessagesTitle">Приветственное сообщение</v:String>
    <v:String x:Key="LocalizableWelcomeMessagesWatermark">Текст сообщения</v:String>
    <v:String x:Key="LocalizableWelcomeMessagesTooltip">Сообщения, которые увидит игрок после присоединения к серверу. Нажмите Enter для создания новой строки.</v:String>

    <v:String x:Key="LocalizableIntervalBetweenMessagesTitle">Интервал между сообщениями</v:String>
    <v:String x:Key="LocalizableIntervalBetweenMessagesTooltip">Интервал между приветственными сообщениями в секундах.</v:String>

    <v:String x:Key="LocalizableHeadlessClientTitle">IP-адреса headless-клиентов</v:String>
    <v:String x:Key="LocalizableLocalClientTitle">IP-адреса локальных клиентов</v:String>
    <v:String x:Key="LocalizableHeadlessClientWatermark">IP-адреса через запятую</v:String>

    <v:String x:Key="LocalizableVotingTresholdTitle">Порог голосования</v:String>
    <v:String x:Key="LocalizableVotingTresholdTooltip">Процент голосов, необходимый для подтверждения голосования.</v:String>

    <v:String x:Key="LocalizableVotingMissionTitle">Голосование за миссию</v:String>
    <v:String x:Key="LocalizableVotingMissionTooltip">Количество игроков на сервере, необходимых для начала голосования за миссию.</v:String>

    <v:String x:Key="LocalizableVotingTimeoutTitle">Перерыв между голосованиями</v:String>
    <v:String x:Key="LocalizableVotingTimeoutTooltip">Перерыв в секундах между голосованиями.</v:String>

    <v:String x:Key="LocalizableRoleSelectionTitle">Время на выбор роли</v:String>
    <v:String x:Key="LocalizableRoleSelectionTooltip">Время в секундах, отведённое на выбор роли в отряде на экране лобби.</v:String>

    <v:String x:Key="LocalizableBriefingTimeoutTitle">Время на брифинг</v:String>
    <v:String x:Key="LocalizableBriefingTimeoutTooltip">Время в секундах, отведённое на брифинг.</v:String>

    <v:String x:Key="LocalizableDebriefingTimeoutTitle">Время на дебрифинг</v:String>
    <v:String x:Key="LocalizableDebriefingTimeoutTooltip">Время в секундах, отведённое на дебрифинг.</v:String>

    <v:String x:Key="LocalizableLobbyIdleTimeoutTitle">Время на неактивнность в лобби</v:String>
    <v:String x:Key="LocalizableLobbyIdleTimeoutTooltip">Время в секундах, отведённое на безделье в лобби.</v:String>

    <v:String x:Key="LocalizableDrawingOnMapTitle">Рисование на карте</v:String>
    <v:String x:Key="LocalizableDrawingOnMapTooltip">Разрешено ли рисование на карте.</v:String>

    <v:String x:Key="LocalizableIsVonEnabledTitle">Выключить голосовую связь</v:String>
    <v:String x:Key="LocalizableIsVonEnabledTooltip">Выключено ли голосовое общение на сервере.</v:String>

    <v:String x:Key="LocalizableVonCodecQualityTitle">Качество голосового кодека</v:String>
    <v:String x:Key="LocalizableVonCodecQualityTooltip">Качество звука голосовых сообщений. 8kHz - значения 0-10, 16kHz - 11-20, 21-30 - 32kHz - SPEEX кодек или 48kHz OPUS кодек.</v:String>

    <v:String x:Key="LocalizableVonCodecTypeTitle">Тип голосового кодека</v:String>

    <v:String x:Key="LocalizablePersistenceTitle">Постоянный сервер</v:String>
    <v:String x:Key="LocalizablePersistenceTooltip">Миссия на сервере будет продолжаться даже если нет ни одного подключенного к серверу игрока.</v:String>

    <v:String x:Key="LocalizableRotorLibTitle">Принудительная RotorLib-симуляция</v:String>
    <v:String x:Key="LocalizableRotorLibTooltip">Принудительно включает физическую симуляцию RotorLib для вертолётов.</v:String>

    <v:String x:Key="LocalizableRequiredBuildTitle">Требуемая сборка игры</v:String>
    <v:String x:Key="LocalizableRequiredBuildTooltip">Определяет версию сборки, при которой можно подключиться к серверу.</v:String>
    <v:String x:Key="LocalizableRequiredBuildWatermark">Версия сборки</v:String>

    <!--  Security view  -->
    <v:String x:Key="LocalizableServerSideScriptingTitle">Серверные скрипты</v:String>
    <v:String x:Key="LocalizableAdministrationTitle">Администрация</v:String>

    <v:String x:Key="LocalizableAdminPasswordTitle">Пароль администратора</v:String>
    <v:String x:Key="LocalizableAdminPasswordWatermark">Пароль для получения прав администратора</v:String>

    <v:String x:Key="LocalizableAdminIdsTitle">ID администраторов</v:String>
    <v:String x:Key="LocalizableAdminIdsWatermark">UIDы через запятую</v:String>
    <v:String x:Key="LocalizableAdminIdsTooltip">Белый список UIDов, которые могут получить доступ к правам администратора на сервере.</v:String>

    <v:String x:Key="LocalizableKickDuplicateUidsTitle">Выгонять игроков с одинаковыми UID</v:String>
    <v:String x:Key="LocalizableKickDuplicateUidsTooltip">Не разрешать игроков с одинаковыми UID. Второй игрок с дублирующим UID будет выброшен из игровой сессии.</v:String>

    <v:String x:Key="LocalizableFilePatchingTitle">Разрешить File Patching</v:String>
    <v:String x:Key="LocalizableFilePatchingTooltip">Разрешает или запрещает File Patching для клиентов.</v:String>

    <v:String x:Key="LocalizableFilePatchingIdsTitle">Исключения File Patching</v:String>
    <v:String x:Key="LocalizableFilePatchingIdsTooltip">Steam ID, которым разрешен  File Patching. Вводить через запятую.</v:String>
    <v:String x:Key="LocalizableFilePatchingIdsWatermark">Steam ID через запятую</v:String>

    <v:String x:Key="LocalizableSignatureVerificationTypeTitle">Тип верификации сигнатур</v:String>
    <v:String x:Key="LocalizableSignatureVerificationTypeTooltip">Тип верификации сигнатур аддонов.</v:String>

    <v:String x:Key="LocalizableServerSidePasswordTitle">Пароль доступа к серверным скриптам</v:String>
    <v:String x:Key="LocalizableDoubleIdCommandTitle">OnDoubleIdCommand</v:String>
    <v:String x:Key="LocalizableDoubleIdCommandTooltip">Выполняется при обнаружении второго клиента с одинаковым ID.</v:String>
    <v:String x:Key="LocalizableUserConnectedCommandTitle">OnUserConnectedCommand</v:String>
    <v:String x:Key="LocalizableUserConnectedCommandTooltip">Выполняется при подключении клиента к серверу.</v:String>
    <v:String x:Key="LocalizableUserDisconnectedCommandTitle">OnUserDisconnectedCommand</v:String>
    <v:String x:Key="LocalizableUserDisconnectedCommandTooltip">Выполняется при отключении клиента от сервера.</v:String>
    <v:String x:Key="LocalizableHackedDataCommandTitle">OnHackedDataCommand</v:String>
    <v:String x:Key="LocalizableHackedDataCommandTooltip">Выполняется в случае, если игровые данные были взломаны.</v:String>

    <v:String x:Key="LocalizableUnsignedDataCommandTitle">OnUsignedDataCommand</v:String>
    <v:String x:Key="LocalizableUnsignedDataCommandTooltip">Выполняется в случае, если игровые данные не подписаны приватным ключом.</v:String>
    <v:String x:Key="LocalizableDifferentDataCommandTitle">OnDifferentDataCommand</v:String>
    <v:String x:Key="LocalizableDifferentDataCommandTooltip">Выполняется в случае, если игровые данные различны между клиентом и сервером.</v:String>
    <v:String x:Key="LocalizableUserKickedCommandTitle">OnUserKickedCommand</v:String>
    <v:String x:Key="LocalizableUserKickedCommandTooltip">Выполняется при выкидывании клиента из игровой сессии.</v:String>

    <v:String x:Key="LocalizableBattleEyeTitle">Battle Eye</v:String>
    <v:String x:Key="LocalizableBattleEyeTooltip">Выключает/выключает Battle-Eye античит.</v:String>



    <!--  Network View  -->
    <v:String x:Key="LocalizableTimeoutHeader">Таймауты, пороговые значения</v:String>

    <v:String x:Key="LocalizableUpnpTitle">UPNP</v:String>
    <v:String x:Key="LocalizableUpnpTooltip">Автоматически назначает порты на UPNP/IGD-роутерах. Эта опция позволяет создавать сервер за NAT.</v:String>

    <v:String x:Key="LocalizableDisconnectTimeoutTitle">Тайм-аут отключения</v:String>
    <v:String x:Key="LocalizableDisconnectTimeoutTooltip">Время ожидания сервера перед отсоединением клиента в секундах.</v:String>

    <v:String x:Key="LocalizableMaxDesyncTitle">Максимальная рассинхронизация</v:String>
    <v:String x:Key="LocalizableMaxDesyncTooltip">Максимальное значение рассинхронизации между клиентом и сервером прежде чем сервер выгонит игрока.</v:String>


    <v:String x:Key="LocalizableMaxPingTitle">Максимальный пинг</v:String>
    <v:String x:Key="LocalizableMaxPingTooltip">Максимальный пинг между клиентом и сервером, прежде чем сервер выгонит игрока.</v:String>

    <v:String x:Key="LocalizableMaxPacketLossTitle">Maximum Packet Loss</v:String>
    <v:String x:Key="LocalizableMaxPacketLossTooltip">Максимальное количество потерянных пакетов между клиентом и сервером, прежде чем сервер выгонит игрока.</v:String>

    <v:String x:Key="LocalizableSlowNetworkRulesTitle">Правила для клиентов с медленным интернетом</v:String>
    <v:String x:Key="LocalizableSlowNetworkRulesWatermark">0 - логгировать, 1 - логгировать и выгонять, 4 значения через запятую</v:String>
    <v:String x:Key="LocalizableSlowNetworkRulesTooltip">Определяет поведения по шаблону {MaxPing, MaxPacketLoss, MaxDesync, DisconnectTimeout}, где 0 - логгирование, 1 - логгирование и удаление клиента из игровой сессии.</v:String>

    <v:String x:Key="LocalizableLanModeTitle">Локальный сервер</v:String>
    <v:String x:Key="LocalizableLanModeTooltip">Сервер будет доступен только из локальной сети.</v:String>

    <!--  Logging view  -->
    <v:String x:Key="LocalizableCallExtensionReportLimitTitle">Лимит логгирования при вызове расширения</v:String>
    <v:String x:Key="LocalizableCallExtensionReportLimitTooltip">Если инициированная сервером команда callExtension занимает больше времени, чем указанное ограничение в миллисекундах, предупреждение будет записано в файл .rpt сервера, а также отражено в результате возврата расширения.</v:String>

    <v:String x:Key="LocalizableLogFileTitle">Вывод серверной консоли</v:String>
    <v:String x:Key="LocalizableLogFileWatermark">Название файла вывода консоли сервера</v:String>
    <v:String x:Key="LocalizableLogFileTooltip">Сохраняет вывод консоли сервера в указанный файл. Файл находится в той же папке, где и краш-дампы и логи. Оставить пустым для выключения сохранения логов консоли.</v:String>

    <!--  Logging view  -->
    <v:String x:Key="LocalizableTerrainGridViewDistanceTitle">Дальность прорисовки ландшафта</v:String>
    <v:String x:Key="LocalizableTerrainGridViewDistanceTooltip">Дальность прорисовки ландшафта в метрах.</v:String>

    <v:String x:Key="LocalizableObjectViewDistanceTitle">Дальность прорисовки объектов</v:String>
    <v:String x:Key="LocalizableObjectViewDistanceTooltip">До какой дистанции объекты будут отрисовываться в метрах.</v:String>

</ResourceDictionary>